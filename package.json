          "id": "apiCenterTreeView",
          "name": "%azure-api-center.views.api-center-treeview.controlplane.title%",
          "icon": "media/api-center-icon.svg",
          "contextualTitle": "Azure API Center"
        },
        {
          "id": "apiCenterWorkspace",
          "name": "%azure-api-center.views.api-center-treeview.dataplane.title%",
          "visibility": "visible"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "api-center-treeview",
          "title": "API Center",
          "icon": "media/api-center-icon.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "azure-api-center.apiCenterTreeView.refresh",
          "when": "view == apiCenterTreeView",
          "group": "navigation"
        },
        {
          "command": "azure-api-center.apiCenterWorkspace.learnApiPortal",
          "when": "view == apiCenterWorkspace",
          "group": "navigation"
        },
        {
          "command": "azure-api-center.apiCenterWorkspace.addApis",
          "when": "view == apiCenterWorkspace",
          "group": "navigation@2"
        },
        {
          "command": "azure-api-center.apiCenterWorkspace.refresh",
          "when": "view == apiCenterWorkspace",
          "group": "navigation@1"
        },
        {
          "command": "azure-api-center.apiCenterWorkspace.collapse",
          "when": "view == apiCenterWorkspace",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "azure-api-center.selectSubscriptions",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterAzureSubscription",
          "group": "inline"
        },
        {
          "command": "azure-api-center.open-api-docs",
          "when": "(view == apiCenterTreeView && viewItem == azureApiCenterApiVersionDefinitionTreeItem-openapi) || (view == apiCenterWorkspace && viewItem == azureApiCenterApiVersionDataPlaneDefinitionTreeItem-openapi)"
        },
        {
          "command": "azure-api-center.open-postman",
          "when": "view == apiCenterTreeView && viewItem =~ /never/"
        },
        {
          "command": "azure-api-center.generate-api-client",
          "when": "(view == apiCenterTreeView && viewItem == azureApiCenterApiVersionDefinitionTreeItem-openapi) || (view == apiCenterWorkspace && viewItem == azureApiCenterApiVersionDataPlaneDefinitionTreeItem-openapi)"
        },
        {
          "command": "azure-api-center.generate-api-plugin",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterApiVersionDefinitionTreeItem-openapi"
        },
        {
          "command": "azure-api-center.importOpenApiByFile",
          "when": "view == apiCenterTreeView && viewItem =~ /never/"
        },
        {
          "command": "azure-api-center.importOpenApiByLink",
          "when": "view == apiCenterTreeView && viewItem =~ /never/"
        },
        {
          "command": "azure-api-center.exportApi",
          "when": "(view == apiCenterTreeView && viewItem =~ /azureApiCenterApiVersionDefinitionTreeItem/) || (view == apiCenterWorkspace && viewItem =~ /azureApiCenterApiVersionDataPlaneDefinitionTreeItem/)"
        },
        {
          "command": "azure-api-center.editApi",
          "when": "view == apiCenterTreeView && viewItem =~ /azureApiCenterApiVersionDefinitionTreeItem/"
        },
        {
          "command": "azure-api-center.generateHttpFile",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterApiVersionDefinitionTreeItem-openapi"
        },
        {
          "command": "azure-api-center.generateMarkdownDocument",
          "when": "(view == apiCenterTreeView && viewItem == azureApiCenterApiVersionDefinitionTreeItem-openapi) || (view == apiCenterWorkspace && viewItem == azureApiCenterApiVersionDataPlaneDefinitionTreeItem-openapi)"
        },
        {
          "command": "azure-api-center.registerApi",
          "when": "view == apiCenterTreeView && viewItem =~ /^azureApiCenterApis/"
        },
        {
          "command": "azure-api-center.registerMCP",
          "when": "view == apiCenterTreeView && viewItem =~ /^azureApiCenterApis/"
        },
        {
          "command": "azure-api-center.createApiEnvironment",
          "when": "view == apiCenterTreeView && viewItem =~ /^azureApiCenterEnvironments/"
        },
        {
          "command": "azure-api-center.searchApi",
          "when": "(view == apiCenterTreeView || view == apiCenterWorkspace) && viewItem =~ /^azureApiCenterApis/ ",
          "group": "inline@0"
        },
        {
          "command": "azure-api-center.cleanupSearchResult",
          "when": "(view == apiCenterTreeView || view == apiCenterWorkspace) && viewItem == azureApiCenterApis-search",
          "group": "inline@1"
        },
        {
          "command": "azure-api-center.exportRules",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterRules",
          "group": "rule@1"
        },
        {
          "command": "azure-api-center.deployRules",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterRules",
          "group": "rule@2"
        },
        {
          "command": "azure-api-center.addCustomFunction",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterFunctions"
        },
        {
          "command": "azure-api-center.renameCustomFunction",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterFunction",
          "group": "function@0"
        },
        {
          "command": "azure-api-center.deleteCustomFunction",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterFunction",
          "group": "function@1"
        },
        {
          "command": "azure-api-center.getCredential",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterApiAccess"
        },
        {
          "command": "azure-api-center.apiCenterWorkspace.removeApi",
          "when": "view == apiCenterWorkspace && viewItem == azureApiCenterDataPlane",
          "group": "inline@0"
        },
        {
          "command": "azure-api-center.createDeclarativeAgent",
          "when": "(view == apiCenterTreeView && viewItem == azureApiCenterApiVersionDefinitionTreeItem-openapi)"
        },
        {
          "command": "azure-api-center.copyDeploymentUrl",
          "when": "(view == apiCenterTreeView && viewItem == azureApiCenterApiDeployment) || (view == apiCenterWorkspace && viewItem == azureApiCenterDataPlaneApiDeployment)"
        },
        {
          "command": "azure-api-center.createIntegrationFromApim",
          "when": "view == apiCenterTreeView && viewItem == azureApiCenterIntegrations"
        },
        {
          "command": "azure-api-center.openInPortal",
          "when": "view == apiCenterTreeView && (viewItem == azureApiCenter || viewItem == azureApiCenterAzureSubscription )",
          "group": "1@1"
        },
        {
          "command": "azure-api-center.createApiCenterService",
          "when": "(view == apiCenterTreeView && viewItem == azureApiCenterAzureSubscription)",
          "group": "2@1"
        }
      ],
      "copilot": [
        {
          "command": "azure-api-center.generateApiFromCode",
          "when": "github.copilot-chat.activated && resourceScheme == 'file'",
          "group": "apiCenter"
        },
        {
          "command": "azure-api-center.generateApiSpecFromCodeProject",
          "when": "github.copilot-chat.activated && workspaceFolderCount >= 1",
          "group": "apiCenter"
        }
      ],
      "commandPalette": [
        {
          "command": "azure-api-center.selectSubscriptions",
          "when": "never"
        },
        {
          "command": "azure-api-center.importOpenApiByFile",
          "when": "never"
        },
        {
          "command": "azure-api-center.importOpenApiByLink",
          "when": "never"
        },
        {
          "command": "azure-api-center.open-postman",
          "when": "never"
        },
        {
          "command": "azure-api-center.open-api-docs",
          "when": "never"
        },
        {
          "command": "azure-api-center.generate-api-client",
          "when": "never"
        },
        {
          "command": "azure-api-center.generate-api-plugin",
          "when": "never"
        },
        {
          "command": "azure-api-center.generateHttpFile",
          "when": "never"
        },
        {
          "command": "azure-api-center.searchApi",
          "when": "never"
        },
        {
          "command": "azure-api-center.cleanupSearchResult",
          "when": "never"
        },
        {
          "command": "azure-api-center.generateApiFromCode",
          "when": "never"
        },
        {
          "command": "azure-api-center.exportRules",
          "when": "never"
        },
        {
          "command": "azure-api-center.deployRules",
          "when": "never"
        },
        {
          "command": "azure-api-center.openRule",
          "when": "never"
        },
        {
          "command": "azure-api-center.addCustomFunction",
          "when": "never"
        },
        {
          "command": "azure-api-center.renameCustomFunction",
          "when": "never"
        },
        {
          "command": "azure-api-center.deleteCustomFunction",
          "when": "never"
        },
        {
          "command": "azure-api-center.apiCenterWorkspace.removeApi",
          "when": "never"
        },
        {
          "command": "azure-api-center.apiCenterWorkspace.collapse",
          "when": "never"
        },
        {
          "command": "azure-api-center.openInPortal",
          "when": "never"
        },
        {
          "command": "azure-api-center.getCredential",
          "when": "never"
        },
        {
          "command": "azure-api-center.copyDeploymentUrl",
          "when": "never"
        },
        {
          "command": "azure-api-center.generateApiSpecFromCodeProject",
          "when": "workspaceFolderCount >= 1"
        }
      ]
    },
    "languageModelTools": [
      {
        "name": "azure-api-center_getSpectralRules",
        "tags": [
          "azure-api-center"
        ],
        "displayName": "%azure-api-center.lmTool.getSpectralRules.displayName%",
        "canBeReferencedInPrompt": true,
        "toolReferenceName": "getSpectralRules",
        "icon": "$(tools)",
        "userDescription": "%azure-api-center.lmTool.getSpectralRules.userDescription%",
        "modelDescription": "Get Spectral rules from active API style guide which makes OpenAPI spec compliant with the API style guide. Whenever user wants to generate or update OpenAPI spec, this tool will be used to fetch the rules.",
        "inputSchema": {}
      },
      {
        "name": "azure-api-center_searchApis",
        "tags": [
          "azure-api-center"
        ],
        "displayName": "%azure-api-center.lmTool.searchApis.displayName%",
        "canBeReferencedInPrompt": true,
        "toolReferenceName": "searchApis",
        "icon": "$(tools)",
        "userDescription": "%azure-api-center.lmTool.searchApis.userDescription%",
        "modelDescription": "Search APIs from Azure API Center.",
        "inputSchema": {}
      },
      {
        "name": "azure-api-center_searchApiVersions",
        "tags": [
          "azure-api-center"
        ],
        "displayName": "%azure-api-center.lmTool.searchApiVersions.displayName%",
        "canBeReferencedInPrompt": true,
        "toolReferenceName": "searchApiVersions",
        "icon": "$(tools)",
        "userDescription": "%azure-api-center.lmTool.searchApiVersions.userDescription%",
        "modelDescription": "Search API versions from Azure API Center.",
        "inputSchema": {
          "type": "object",
          "properties": {
            "apiName": {
              "type": "string",
              "description": "API name"
            }
          }
        }
      },
      {
        "name": "azure-api-center_searchApiDefinitions",
        "tags": [
          "azure-api-center"
        ],
        "displayName": "%azure-api-center.lmTool.searchApiDefinitions.displayName%",
        "canBeReferencedInPrompt": true,
        "toolReferenceName": "searchApiDefinitions",
        "icon": "$(tools)",
        "userDescription": "%azure-api-center.lmTool.searchApiDefinitions.userDescription%",
        "modelDescription": "Search API definitions from Azure API Center.",
        "inputSchema": {
          "type": "object",
          "properties": {
            "apiName": {
              "type": "string",
              "description": "API name"
            },
            "apiVersionName": {
              "type": "string",
              "description": "API version name"
            }
          }
        }
      },
      {
        "name": "azure-api-center_exportApiSpecification",
        "tags": [
          "azure-api-center"
        ],
        "displayName": "%azure-api-center.lmTool.exportApiSpecification.displayName%",
        "canBeReferencedInPrompt": true,
        "toolReferenceName": "exportApiSpecification",
        "icon": "$(tools)",
        "userDescription": "%azure-api-center.lmTool.exportApiSpecification.userDescription%",
        "modelDescription": "Export API specification from Azure API Center.",
        "inputSchema": {
          "type": "object",
          "properties": {
            "apiName": {
              "type": "string",
              "description": "API name"
            },
            "apiVersionName": {
              "type": "string",
              "description": "API version name"
            },
            "apiDefinitionName": {
              "type": "string",
              "description": "API definition name"
            }
          }
        }
      },
      {
        "name": "azure-api-center_searchApiDeployments",
        "tags": [
          "azure-api-center"
        ],
        "displayName": "%azure-api-center.lmTool.searchApiDeployments.displayName%",
        "canBeReferencedInPrompt": true,
        "toolReferenceName": "searchApiDeployments",
        "icon": "$(tools)",
        "userDescription": "%azure-api-center.lmTool.searchApiDeployments.userDescription%",
        "modelDescription": "Search API deployments under an API from Azure API Center.",
        "inputSchema": {
          "type": "object",
          "properties": {
            "apiName": {
              "type": "string",
              "description": "API name"
            }
          }
        }
      }
    ],
    "problemMatchers": [
      {
        "name": "optic",
        "owner": "api-center",
        "fileLocation": [
          "autoDetect",
          "${workspaceFolder}"
        ],
        "pattern": [
          {
            "regexp": "x (.*breaking change.*)",
            "message": 1
          },
          {
            "regexp": "at (.*):(\\d+):(\\d+)",
            "file": 1,
            "line": 2
          }
        ]
      }
    ],
    "configuration": [
      {
        "title": "Azure Api Center",
        "properties": {
          "azure-api-center.showSavePrompt": {
            "type": "boolean",
            "default": true,
            "description": "Show warning dialog on remote file uploading."
          },
          "azure-api-center.selectedSubscriptions": {
            "type": "array",
            "description": "Selected Azure subscriptions",
            "items": {
              "type": "string"
            }
          },
          "azure-api-center.tenant": {
            "type": "object",
            "description": "A specific tenant to sign in to",
            "properties": {
              "name": {
                "type": "string",
                "description": "tenant name"
              },
              "id": {
                "type": "string",
                "description": "tenant id"
              }
            }
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out && copyfiles -u 1 \"src/test/resources/**/*\" out/",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test --coverage",
    "prepare": "husky install",
    "lint:staged": "lint-staged",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@azure/openai": "^2.0.0",
    "@playwright/test": "^1.42.1",
    "@stoplight/spectral-core": "^1.19.4",
    "@stoplight/spectral-formats": "^1.8.2",
